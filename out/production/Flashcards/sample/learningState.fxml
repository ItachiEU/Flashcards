<AnchorPane prefHeight="500.0" prefWidth="800.0" styleClass="anchorLearn" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LearningStateController">
    <children>
        <Button fx:id="backToMenu" layoutX="695.0" layoutY="460.0" mnemonicParsing="false" onAction="#backToMainMenu" text="Main menu" />
        <ListView fx:id="setList" layoutX="14.0" layoutY="146.0" prefHeight="258.0" prefWidth="190.0" />
        <Label layoutX="14.0" layoutY="115.0" prefHeight="16.0" prefWidth="183.0" text="Choose set to learn from:" />
        <Rectangle fx:id="rectangleShape" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="285.0" layoutX="263.0" layoutY="117.0" stroke="BLACK" strokeType="INSIDE" width="516.0" />
        <Button fx:id="knowButton" layoutX="283.0" layoutY="356.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="185.0" text="I know this one!" />
        <Button fx:id="explainButton" layoutX="564.0" layoutY="356.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="185.0" text="Show me the explanation...">
            <font>
                <Font size="12.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="106.0" layoutY="36.0" prefHeight="52.0" prefWidth="573.0" text="TIME TO LEARN! :)">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <Label fx:id="flashcardLabel" alignment="CENTER" layoutX="330.0" layoutY="179.0" prefHeight="146.0" prefWidth="385.0" text="Label" textAlignment="CENTER">
            <font>
                <Font size="40.0" />
            </font>
        </Label>
      <Button fx:id="nextButton" layoutX="474.0" layoutY="356.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="82.0" text="Next" />
    </children>
</AnchorPane>
